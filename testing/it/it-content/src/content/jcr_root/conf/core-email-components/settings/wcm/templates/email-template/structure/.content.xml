<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/core-email-components/settings/wcm/templates/email-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="core/email/components/page/v1/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <experiencefragment_header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/email/components/experiencefragment/v1/experiencefragment"
                editable="{Boolean}true"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/email/components/container/v1/container"
                layout="3-3">
                <col-0
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"
                    editable="{Boolean}true"/>
                <col-1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"
                    editable="{Boolean}true"/>
                <col-2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"
                    editable="{Boolean}true"/>
            </container>
            <experiencefragment_footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/email/components/experiencefragment/v1/experiencefragment"
                editable="{Boolean}true"/>
        </root>
    </jcr:content>
</jcr:root>
